{"code":"import { isObject } from '@vue/shared';\nimport { reactive, readonly } from './reactive';\nfunction createGetter(isReadOnly = false, shall = false) {\n    return function get(target, key, receiver) {\n        let res = Reflect.get(target, key, receiver);\n        if (!isReadOnly) {\n            // 收集依赖\n        }\n        if (shall) { // 浅\n            return res;\n        }\n        if (isObject(res)) { // 懒代理 { list: { name: 'wenji' }} 懒代理.list对象，\n            // 不使用数据，不取到.list,.list就不会生成proxy对象，懒代理\n            return isReadOnly ? readonly(res) : reactive(res);\n        }\n        return res;\n    };\n}\nconst get = () => createGetter();\nconst shallowReactiveGet = () => createGetter(false, true);\nconst shallowReadonlyGet = () => createGetter(true, true);\nconst readonlyGet = () => createGetter(true, false);\nconst reactiveHandlers = {\n    get,\n    set() {\n    }\n};\nconst shallowReactiveHandlers = {\n    get: shallowReactiveGet,\n};\nconst shallowReadonlyHandlers = {\n    get: shallowReadonlyGet,\n    set: (target, key, value) => {\n        console.log(`set key on fail:${target}.${key}`);\n    }\n};\nconst readonlyHandlers = {\n    get: readonlyGet,\n    set: (target, key, value) => {\n        console.log(`set key on fail:${target}.${key}`);\n    }\n};\nexport { reactiveHandlers, shallowReactiveHandlers, shallowReadonlyHandlers, readonlyHandlers };\n//# sourceMappingURL=baseHandler.js.map","references":["D:/own-demo/exercise/vue3-code/packages/shared/src/index.ts","D:/own-demo/exercise/vue3-code/packages/reactivity/src/reactive.ts"],"map":"{\"version\":3,\"file\":\"baseHandler.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/reactivity/src/baseHandler.ts\"],\"names\":[],\"mappings\":\"AAEA,OAAO,EAAE,QAAQ,EAAE,MAAM,aAAa,CAAA;AACtC,OAAO,EACL,QAAQ,EAGR,QAAQ,EACT,MAAM,YAAY,CAAA;AAEnB,SAAS,YAAY,CAAE,UAAU,GAAG,KAAK,EAAE,KAAK,GAAG,KAAK;IACtD,OAAO,SAAS,GAAG,CAAC,MAAM,EAAE,GAAG,EAAE,QAAQ;QACvC,IAAI,GAAG,GAAG,OAAO,CAAC,GAAG,CAAC,MAAM,EAAE,GA