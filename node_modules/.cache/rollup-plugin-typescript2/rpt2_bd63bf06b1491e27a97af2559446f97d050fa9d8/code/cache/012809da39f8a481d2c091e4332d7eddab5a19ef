{"code":"import { isObject } from '@vue/shared';\nimport { reactiveHandlers, shallowReactiveHandlers, shallowReadonlyHandlers, readonlyHandlers } from './baseHandler';\nconst reactiveMap = new WeakMap();\nconst readonlyMap = new WeakMap();\nfunction createReactiveObj(target, isReadOnly, baseHandlers) {\n    if (!isObject(target)) {\n        return target;\n    }\n    const map = isReadOnly ? readonlyMap : reactiveMap;\n    let targetProxy = map.get(target);\n    if (targetProxy) {\n        return targetProxy;\n    }\n    const proxy = new Proxy(target, baseHandlers);\n    map.set(target, proxy);\n    return proxy;\n}\nfunction reactive(target) {\n    return createReactiveObj(target, false, reactiveHandlers);\n}\nfunction shallowReactive(target) {\n    return createReactiveObj(target, false, shallowReactiveHandlers);\n}\nfunction shallowReadonly(target) {\n    return createReactiveObj(target, false, shallowReadonlyHandlers);\n}\nfunction readonly(target) {\n    return createReactiveObj(target, true, readonlyHandlers);\n}\nlet a = reactive({ name: 'wenji' });\nconsole.log(a);\nconsole.log(a.name);\n//# sourceMappingURL=index.js.map","references":["D:/own-demo/exercise/vue3-code/packages/shared/src/index.ts","D:/own-demo/exercise/vue3-code/packages/reactivity/src/baseHandler.ts"],"map":"{\"version\":3,\"file\":\"index.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/reactivity/src/index.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,QAAQ,EAAE,MAAM,aAAa,CAAA;AACtC,OAAO,EAAE,gBAAgB,EAAE,uBAAuB,EAAE,uBAAuB,EAAE,gBAAgB,EAAE,MAAM,eAAe,CAAA;AAEpH,MAAM,WAAW,GAAG,IAAI,OAAO,EAAE,CAAA;AACjC,MAAM,WAAW,GAAG,IAAI,OAAO,EAAE,CAAA;AAEjC,SAAS,iBAAiB,CAAC,MAAM,EAAC,UAAU,EAAC,YAAY;IACvD,IAAI,CAAC,QAAQ,CAAC,MAAM,CAAC,EAAE;QACrB,OAAO,MAAM,CAAA;KACd;IACD,MAAM,GAAG,GAAG,UAAU,CAAC,CAAC,CAAC,WAAW,CAAC,CAAC,CAAC,WAAW,CAAA;IAClD,IAAI,WAAW,GAAG,GAAG,CAAC,GAAG,CAAC,MAAM,CAAC,CAAA;IACjC,IAAI,WAAW,EAAE;QACf,OAAO,WAAW,CAAA;KACnB;IACD,MAAM,KAAK,GAAG,IAAI,KAAK,CAAC,MAAM,EAAE,YAAY,CA